<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shoppinglist_zeeshan">

    <application
        android:allowBackup="true"
        android:icon="@drawable/shopping_cart"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ShoppingList_Zeeshan">
        <activity android:name=".WelcomeScreen.ViewPagerScreen"></activity>
        <activity android:name=".ArchieveActivity" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateAccount" />
        <activity android:name=".LogIn" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>